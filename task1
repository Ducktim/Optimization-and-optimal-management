        "ax = fig.gca(projection=\"3d\")\n",
        "\n",
        "xmesh, ymesh = np.mgrid[-5:5:50j,-5:5:50j]\n",
        "fmesh = f(np.array([xmesh, ymesh]))\n",
        "ax.plot_surface(xmesh, ymesh, fmesh)"
